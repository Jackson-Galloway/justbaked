<?xml version="1.0"?>
<robot name="justbaked">

<link name="base_link">
    <visual name="frame">
        <origin xyz="0.0 0.0 0.16666665" rpy="0.0 0.0 0.0"/>
        <geometry>
            <box size="1.0 1.0 0.3333333"/>
        </geometry>
        <material name="red">
            <color rgba="1.0 0.0 0.0 0.9"/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
        <geometry>
            <box size="1.0 1.0 0.3333333"/>
        </geometry>
    </collision>
</link>

<joint name="joint_name" type="continuous">
    <origin xyz="0.0 0.0 0.3661417" rpy="0.0 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="lidar"/>
    <axis xyz="0.0 0.0 1.0"/>
</joint>

<link name="lidar">
    <visual name="lidar">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.229659" length="0.0656168"/>
        </geometry>
        <material name="white">
            <color rgba="1.0 1.0 1.0 1.0"/>
        </material>
    </visual>
</link>


</robot>