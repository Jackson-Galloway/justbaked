<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

<link name="base_link">
    
</link>

<!-- Chassis  -->

<joint name="chassis_joint" type="fixed">
    <origin xyz="-0.1 0.0 0.05"/>
    <parent link="base_link"/>
    <child link="chassis"/>
</joint>

<link name="chassis">
    <visual name="chassis">
        <origin xyz="0.0 0.0 0.16666665" rpy="0.0 0.0 0.0"/>
        <geometry>
            <box size="1.0 0.8 0.3333333"/>
        </geometry>
        <material name="red">
            <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
        <geometry>
            <box size="1.0 1.0 0.3333333"/>
        </geometry>
    </collision>
    <inertial>
        <origin xyz="0.0 0.0 0.16666665" rpy="0.0 0.0 0.0"/>
        <mass value="10.0"/>
        <inertia ixx="0.625924" ixy="0" ixz="0" iyy="1.36667" iyz="0" izz="0.92592"/>
    </inertial>
</link>

<!-- Front left wheel -->
<joint name="front_left_wheel_joint" type="continuous">
    <origin xyz="0.4 0.45 0.05" rpy="${pi/2} 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="front_left_wheel"/>
    <axis xyz="0.0 0.0 1.0"/>
</joint>

<link name="front_left_wheel">
    <visual name="front_left_wheel">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.1" length="0.1"/>
        </geometry>
        <material name="black">
            <color rgba="0.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="1.0"/>
        <inertia ixx="0.003333333" ixy="0" ixz="0" iyy="0.003333333" iyz="0" izz="0.005"/>
    </inertial>
</link>

<!-- Rear left wheel -->
<joint name="rear_left_wheel_joint" type="continuous">
    <origin xyz="-0.4 0.45 0.05" rpy="${pi/2} 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="rear_left_wheel"/>
    <axis xyz="0.0 0.0 1.0"/>
</joint>

<link name="rear_left_wheel">
    <visual name="rear_left_wheel">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.1" length="0.1"/>
        </geometry>
        <material name="black">
            <color rgba="0.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="1.0"/>
        <inertia ixx="0.003333333" ixy="0" ixz="0" iyy="0.003333333" iyz="0" izz="0.005"/>
    </inertial>
</link>

<!-- Front right wheel -->
<joint name="front_right_wheel_joint" type="continuous">
    <origin xyz="0.4 -0.45 0.05" rpy="${pi/2} 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="front_right_wheel"/>
    <axis xyz="0.0 0.0 1.0"/>
</joint>

<link name="front_right_wheel">
    <visual name="front_right_wheel">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.1" length="0.1"/>
        </geometry>
        <material name="black">
            <color rgba="0.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="1.0"/>
        <inertia ixx="0.003333333" ixy="0" ixz="0" iyy="0.003333333" iyz="0" izz="0.005"/>
    </inertial>
</link>

<!-- Rear right wheel -->
<joint name="rear_right_wheel_joint" type="continuous">
    <origin xyz="-0.4 -0.45 0.05" rpy="${pi/2} 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="rear_right_wheel"/>
    <axis xyz="0.0 0.0 1.0"/>
